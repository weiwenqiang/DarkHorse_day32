<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <Button 
        android:id="@+id/b1_start"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b1_start"/>
    <Button 
        android:id="@+id/b1_stop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b1_stop"/>
    <Button 
        android:id="@+id/b1_bind"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b1_bind"/>
    <Button 
        android:id="@+id/b1_unbind"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b1_unbind"/>
    
    <Button 
        android:id="@+id/b1_banzheng"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/b1_banzheng"/>

</LinearLayout>
